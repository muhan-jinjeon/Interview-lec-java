package logic;
//1-2 +3-4 + 5-6 = -3
public class Series3 {

	public static void main(String[] args) {
		int n=6, num1=0,num2=0,num3,num4, count=0;
		System.out.println("First " +n+ " numbers");

		for(int i=1 ; i<=n ;i++) {
			if(n>=1)
				num1=n+i;
			else {
				num2=n+i;
			}
			System.out.println(" "+num1"+"+num2);
			
		}
			
			
		//count= -(n/2);
		System.out.println( "=" + (count) );	

		}
		
		

	}


